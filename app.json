{
  "expo": {
    "name": "Crypto Wallet",
    "slug": "tci-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cryptologo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cryptologo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.andiamsam.tciapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9bd5cc02-bbc2-4429-8586-1fc79e4e55ae"
      }
    }
  }
}
